                {
                  "id": "eca1184e-0fdd-4168-a59c-15a45e6e687a",
                  "title": "Linking child accounts and activity across platforms",
                  "status": "Open",
                  "severity": "Medium",
                  "type": "Linkability",
                  "description": "Sharing videos with friends, family, etc. could lead to linking the user's accounts across multiple platforms or devices, enabling profiling",
                  "mitigation": "Account on KidsTube should not be linked directly to the child, and instead be necessary to go through a parent/guardian. Sharing through a third party should not be possible for children, and instead sharing should be encouraged via copying link.",
                  "modelType": "LINDDUN",
                  "new": false,
                  "number": 4,
                  "score": ""
                },
                {
                  "id": "eb316304-1a31-412f-be17-989e2b191740",
                  "title": "Sharing viewing data and targeted recommendations",
                  "status": "Open",
                  "severity": "Medium",
                  "type": "Linkability",
                  "description": "The recommendation algorithm linking viewing history to generate recommendations, and sharing viewing data with third-party analytics could reveal connections between videos watched and enable profiling.",
                  "mitigation": "Target recommendations should not involve third parties, and instead only be conducted in-app. There should be a strict restriction on parent/guardian approval for registration, and data should not be linked to any specific child.",
                  "modelType": "LINDDUN",
                  "new": false,
                  "number": 5,
                  "score": ""
                }
